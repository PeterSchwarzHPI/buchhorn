<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="real_estate_building_heating_type_view_form" model="ir.ui.view">
        <field name="name">real.estate.building.heating.type.view.form</field>
        <field name="model">real.estate.building.heating_type</field>
        <field name="arch" type="xml">
            <form string="Building Heating Type">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                        <group/>
                    </group>
                </sheet>
                <chatter/>
            </form>
        </field>
    </record>
    <record id="real_estate_building_heating_type_view_list" model="ir.ui.view">
        <field name="name">real.estate.building.heating.type.view.list</field>
        <field name="model">real.estate.building.heating_type</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>
    <record id="real_estate_building_heating_type_action" model="ir.actions.act_window">
        <field name="name">Building Heating Types</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">real.estate.building.heating_type</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>
